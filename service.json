{
    "id": "rw_adapter_cartodb_v1",
    "name": "RW Adapter CartoDB v1",
    "urls": [{
        "url": "/query/:idDataset",
        "method": "GET",
        "paramProvider": "idDataset",
        "dataProvider": "dataset",
        "filters":{
            "provider": "CartoDb"
        },
        "endpoints": [{
            "method": "POST",
            "baseUrl": "#(service.uri)",
            "path": "/query/:idDataset",
            "data": ["dataset"]
        }]
    }]
}
